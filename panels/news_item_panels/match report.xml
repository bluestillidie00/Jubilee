<panel>
	<layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true"/>

	<!-- Background image layer -->
	<container id="laye">
		<layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true"/>
		
		<container class="client_object_viewer_xml_panel" id="bgim">
			<layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true"/>

			<record id="object_property" get_property="comp" set_property="objt"/>
			
			<widget class="value_based_picture" id="CoPi" image_alignment="centre" keep_aspect_ratio="true" scale_picture="true" >
				<record id="object_property">
					<integer id="get_property" value="id  " />
					<integer id="set_property" value="valu" />
				</record>

				<list id="value_list">
					<record value="1301394" file="photos/dynamic/uefa/pre match/ucl pre-game"/>
					<record value="1301396" file="photos/dynamic/uefa/pre match/uel pre-game"/>
					<record value="31051584" file="photos/dynamic/uefa/pre match/uecl pre-game"/>
					<record value="1301397" file="photos/dynamic/uefa/pre match/usc pre-game"/>
				</list>
			</widget>

		</container>
	</container>

	<container >
		<attachment_group class="horizontal_arrange" horizontal_layout="-4,-3,-4" horizontal_offset="4" horizontal_gap="0" vertical_inset="0"/>


		<container>
			<attachment_group class="vertical_arrange" vertical_layout="-1,-3" horizontal_inset="10"/>

			<container/>
			<!-- Logo/Portrait -->
			<widget class="object_portrait_picture" id="log1" image_alignment="centre" scale_picture="true" keep_aspect_ratio="true" wants_mouse_events="false">
				<!-- PROP_TEAM -->
				<record id="object_property" get_property="Ttea" set_property="objt"/>
			</widget>
		</container>

		<container>
			<attachment_group class="vertical_arrange" vertical_layout="-1,-4,-3,-1"/>

			<container/>

			<container class="client_object_viewer_xml_panel" >
				<record id="object_property" get_property="comp" set_property="objt"/>

				<layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true"/>
				<widget class="value_based_picture" id="vbPI" image_alignment="centre" keep_aspect_ratio="true" scale_picture="true" >
					<record id="object_property">
						<integer id="get_property" value="id  " />
						<integer id="set_property" value="valu" />
					</record>

					<list id="value_list">
						<record value="1301394" file="pictures/logos/custom/comps/eur/ucl_full"/>
						<record value="1301396" file="pictures/logos/custom/comps/eur/uel_full"/>
						<record value="31051584" file="pictures/logos/custom/comps/eur/uecl_full"/>
						<record value="1301397" file="pictures/logos/custom/comps/eur/usc_full"/>
					</list>
				</widget>


			</container>

			<container>
				<layout class="stick_to_sides_attachment" alignment="all" inset="10,0,0,0" apply_to_children="true"/>
				<widget class="text" id="goal" alignment="centre,auto_fit" colour="white" font="match report score" size="30">
					<colour id="link_colour" red="0" green="0" blue="0" alpha="1"/>
					<record id="object_property">
						<list id="get_properties">
							<record get_property="text" set_property="text"/>
						</list>
					</record>
				</widget>
			</container>

			<container/>
			
		</container>


		<container>
			<attachment_group class="vertical_arrange" vertical_layout="-1,-3" horizontal_inset="10"/>

			<container/>
			<widget class="object_portrait_picture" id="log2" image_alignment="centre" scale_picture="true" keep_aspect_ratio="true" wants_mouse_events="false">
				<!-- PROP_OTHER_TEAM -->
				<record id="object_property" get_property="ottm" set_property="objt"/>
			</widget>
		</container>



	</container>

</panel>