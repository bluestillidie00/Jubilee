<panel>
	<layout class="stick_to_sides_attachment" apply_to_children="true" inset="8,4,4,4"/>
	<boolean id="should_force_refresh" value="true" />
	<list id="properties_to_forward">
		<record get_property="recommendation_widget_mode" set_property="recommendation_mode" target="sriW" />
		<record get_property="showing_in_list" set_property="showing_in_list" target="sriW" />
	</list>

	<!-- IF WE DON'T HAVE PLAYER -->
	<!-- ADAPTIVE CONTAINER -->
	<container class="horizontal_adaptive_container" gap="8">

		<!-- IF AI CONTROLLED TEAM, SHOW -->
		<record id="object_property" get_property="PHcr" set_property="hidn"/>

		<!-- SCOUT REPORT -->
		<container default_width="-66" class="player_scout_reports_panel" type="1" file="none" priority="1">
			<layout class="stick_to_sides_attachment" apply_to_children="true"/>

			<boolean id="use_pro_con_long_strings" value="true"/>

			<record id="object_property" get_property="objt" set_property="objt"/>

			<!-- NO SCOUT REPORT -->
			<container class="tcs_box" id="nsrc">
				<translation id="title" translation_id="316845" type="use" />
				<attachment_group class="vertical_arrange" vertical_alignment="middle" />

				<container id="nocs" height="70">
					<attachment_group class="vertical_arrange_autosize" />

					<!--EXPLANATORY TEXT-->
					<widget class="label" alignment="centre" spec="text" size="11" auto_size="vertical">
						<translation id="text" translation_id="320496" type="use" />
					</widget>

					<!-- BUTTON -->
					<container height="30">
						<attachment_group class="horizontal_arrange" horizontal_alignment="middle" />

						<widget class="button" id="sctP" click_event="sctc" alignment="centre" auto_size="horizontal" height="30">
							<translation id="text" translation_id="243421" type="use"/>
						</widget>
					</container>
				</container>

				<widget id="nocn" class="label" spec="text" size="11" alignment="centre" auto_size="vertical">
					<layout class="stick_to_sides_attachment" alignment="horizontal" inset="0" />
				</widget>
			</container>

			<!-- Already being scounted -->
			<container id="Arbs" class="main_box">
				<layout class="stick_to_sides_attachment" alignment="top, horizontal" apply_to_children="true" inset="0" />

				<!--explanatory text-->
				<widget class="text" id="arbt" alignment="centre" spec="text" colour="attention" height="80">
					<translation id="text" translation_id="447600" type="use" value="This player is being scouted by [%person#1] and the report will be available shortly.[COMMENT:scout report, scouting in progress]" />
				</widget>
			</container>

			<!-- Already being scouted by pool -->
			<container id="Pool" class="main_box">
				<layout class="stick_to_sides_attachment" alignment="top, horizontal" apply_to_children="true" inset="0" />

				<!--explanatory text-->
				<widget class="text" alignment="centre" spec="text" size="large" style="semi_bold, embossed" height="80">
					<translation id="text" translation_id="342085" type="use" value="Scouting In Progress[COMMENT: disabled menu item when a player is already being scouted]" />
				</widget>
			</container>

			<!-- IF WE HAVE A REPORT -->
			<container class="horizontal_adaptive_container" gap="8">

				<record id="default_properties" default_width="-1"/>

				<!-- OVERVIEW -->
				<container class="scrolling_box">
					<container class="tcs_box">
						<layout class="stick_to_sides_attachment" apply_to_children="true"/>

						<container class="player_reports_overview_summary_panel" id="proS" file="none" always_recolour_wage_cost_text="true" recommendation_widget_mode="0" mode="1">
							<attachment_group class="vertical_arrange" vertical_alignment="top,extend" vertical_gap="10" />

							<record id="object_property" get_property="objt" set_property="objt"/>

							<!-- LAST SCOUTED BY -->
							<container>
								<attachment_group class="horizontal_arrange_vertical_autosize_centre" horizontal_alignment="right,extend" horizontal_offset="0" vertical_gap="0" vertical_offset="0" />

								<!-- SCOUT REPORT, LAST SCOUTED BY -->
								<container>
									<attachment_group class="vertical_arrange_autosize" vertical_gap="2" vertical_offset="0" />

									<widget class="label" id="perl" size="normal" colour="contrast" auto_size="all" font="title">
										<translation id="text" translation_id="316845" type="use" value="Scout Report[COMMENT: Panel title for when viewing a scout report]" />
									</widget>

									<widget class="text" id="lsdt" auto_size="all"/>
								</container>

								<!-- KNOWLEDGE LEVEL -->
								<container width="400">
									<attachment_group class="vertical_arrange_autosize" horizontal_alignment="right" />
									<layout class="stick_to_sides_attachment" alignment="right" inset="10" />

									<widget class="icon_button" id="khtx" appearance="" icon="icons/16px/scouting" font="label" colour="fg" auto_size="all" size="medium" />
									<widget class="label" id="rdtx" colour="disabled" auto_size="all" size="medium" />
								</container>
							</container>

							<!-- TOP BAR -->
							<container id="stBX" class="main_contrast_box_no_margin">
								<attachment_group class="horizontal_arrange_vertical_autosize_centre" horizontal_layout="44,-6, -1" horizontal_gap="4" horizontal_offset="6" vertical_offset="8" />

								<!-- RECOMMENDATION LETTER -->
								<widget class="player_scout_recommendation_widget" id="sriW" width="40" height="40" show_knowledge_bar="false" navigation_focus_target="false">
									<record id="object_property" get_property="PRpo" set_property="objt" />
								</widget>

								<!-- TEXT COLOUR -->
								<widget class="text" id="recw" alignment="left,centre_y" multiline="true" style="bold" auto_size="vertical" />
							</container>

							<!-- STARS AND TRANSFER INFO -->
							<container draw_horizontal_dividers="true">
								<attachment_group class="vertical_arrange_autosize" vertical_gap="24" />

								<!-- STARS -->
								<container id="strs">
									<attachment_group class="vertical_arrange_autosize" vertical_offset="0" vertical_gap="12" />

									<!-- KEY INFO LABEL -->
									<widget class="label" font="title" alignment="left,centre_y" auto_size="vertical">
										<translation id="text" translation_id="539029" type="use" />
									</widget>

									<!-- CURRENT ABILITY -->
									<container>
										<attachment_group class="vertical_arrange_autosize" vertical_offset="0" vertical_gap="6" />

										<!-- CA LABEL -->
										<widget class="text" auto_size="vertical" spec="text" font="label" colour="text" alignment="left,centre_y">
											<translation id="text" translation_id="251893" type="use" />
										</widget>

										<!-- CA STARS -->
										<container>
											<attachment_group class="horizontal_arrange_vertical_autosize_centre" horizontal_layout="80, -1" />

											<!-- STARS -->
											<widget class="scout_stars_label" id="cast" height="18" alignment="left, centre_y" mode="1">
												<record id="object_property" get_property="Pcab" />
											</widget>

											<!-- LABEL -->
											<widget class="text" id="caLB" auto_size="vertical" alignment="left" colour="faded text" />
										</container>
									</container>

									<!-- POTENTIAL ABILITY -->
									<container>
										<attachment_group class="vertical_arrange_autosize" vertical_offset="0" vertical_gap="6" />

										<!-- POTENTIAL ABILITY LABEL -->
										<widget class="text" auto_size="vertical" spec="text" font="label" colour="text" alignment="left,centre_y">
											<translation id="text" translation_id="251894" type="use"/>
										</widget>

										<!-- PA STARS -->
										<container>
											<attachment_group class="horizontal_arrange_vertical_autosize_centre" horizontal_layout="80, -1" />

											<!-- PA STARS -->
											<widget class="scout_stars_label" id="past" height="18" alignment="left" mode="2">
												<record id="object_property" get_property="Ppab" />
											</widget>

											<!-- PA LABEL -->
											<widget class="text" id="paLB" auto_size="vertical" alignment="left" colour="faded text" />
										</container>
									</container>
								</container>

								<!-- TRANSFER INFORMATION -->
								<container>
									<attachment_group class="vertical_arrange_autosize" vertical_gap="8"/>

									<!-- PLAYER VALUE AND WAGE -->
									<container>
										<attachment_group class="horizontal_arrange_vertical_autosize" horizontal_alignment="fill" />

										<!-- TRANSFER VALUE -->
										<container>
											<attachment_group class="vertical_arrange_autosize" vertical_offset="0" vertical_gap="3" />


											<!-- LABEL -->
											<widget class="text" font="label" colour="text" auto_size="vertical">
												<translation id="text" translation_id="538841" type="use"  />
											</widget>

											<!-- ESTIMATED VALUE -->
											<widget class="text" id="cosw" style="semi_bold" auto_size="vertical" multiline="false" />
										</container>

										<!-- WAGE -->
										<container>
											<attachment_group class="vertical_arrange_autosize" vertical_offset="0" vertical_gap="3" />


											<!-- LABEL -->
											<widget class="text" font="label" colour="text" auto_size="vertical">
												<translation id="text" translation_id="450789" type="use" />
											</widget>

											<!-- ESTIMATED VALUE -->
											<container>
												<attachment_group class="vertical_arrange_autosize" vertical_offset="0" vertical_gap="0" />


												<widget class="text" id="ewaw" style="semi_bold" auto_size="vertical" multiline="false" />
												<widget class="text" id="wagw" auto_size="vertical" colour="faded text" />
											</container>
										</container>
									</container>

									<!-- TRANSFER AND LOAN INTEREST -->
									<container>
										<attachment_group class="horizontal_arrange_vertical_autosize" horizontal_alignment="fill" />

										<!-- TRANSFER INTEREST -->								
										<container id="trif">
											<attachment_group class="vertical_arrange_autosize" vertical_offset="0" vertical_gap="3" />


											<widget class="text" id="trit" font="label" colour="text" auto_size="vertical">
												<translation id="text" translation_id="450790" type="use" value="Transfer Interest[COMMENT: FR 21116, scout report panel label]" />
											</widget>

											<widget class="text" id="triw" style="semi_bold" auto_size="vertical" multiline="false" />
										</container>

										<!-- WAGE INTEREST -->
										<container id="loif">
											<attachment_group class="vertical_arrange_autosize" vertical_offset="0" vertical_gap="3" />


											<widget class="text" id="loit" font="label" colour="text" auto_size="vertical">
												<translation id="text" translation_id="450791" type="use" value="Loan Interest[COMMENT: FR 21116, scout report panel label]" />
											</widget>

											<widget class="text" id="loiw" style="semi_bold" auto_size="vertical" multiline="false" />
										</container>
									</container>

									<widget class="horizontal_subtle_divider" height="2"/>
								</container>
							</container>

							<container id="sqdC">
								<attachment_group class="vertical_arrange" vertical_alignment="top,extend" vertical_offset="0" horizontal_inset="0"/>

								<widget class="link_button_title" id="sqcw" size="medium" alignment="left, centre_y">
									<string id="remove_if" value="is_fmt"/>
								</widget>
								<widget class="text" id="sqcw" font="title" size="medium" auto_size="vertical">
									<string id="create_if" value="is_fmt"/>
								</widget>

								<!-- Ability list table -->
								<widget class="table" id="calL" mode="fill_rows,stripe_rows" layout="26,-5,-3,-3">
									<list id="column_widget_properties">
										<record indx="0" alignment="left,centre_y" colour="faded text" />
										<record indx="1" alignment="left,centre_y" />
										<record indx="2" alignment="left,centre_y" navigation_focus_target="false" />
										<record indx="3" alignment="left,centre_y" navigation_focus_target="false" />
									</list>

									<list id="column_table_properties">
										<record indx="0" sort_disabled="true" left="5" />
										<record indx="1" sort_disabled="true" >
											<record id="column_heading">
												<flags id="alignment" value="left,centre_y" />
												<translation id="text" translation_id="227966" type="use" value="Player" />
											</record>
										</record>
										<record indx="2" sort_disabled="true" >
											<record id="column_heading">
												<flags id="alignment" value="left,centre_y" />
												<translation id="text" translation_id="314426" type="use" value="Ability[COMMENT: player popup info panel - scout current ability rating]" />
												<translation id="hint" translation_id="310135" type="use" value="Current Ability[COMMENT: contract offer screen - comments table]" />
											</record>
										</record>
										<record indx="3" sort_disabled="true" >
											<record id="column_heading">
												<flags id="alignment" value="left,centre_y" />
												<translation id="text" translation_id="314427" type="use" value="Potential[COMMENT: player popup info panel - scout potential ability rating]"/>
												<translation id="hint" translation_id="310136" type="use" value="Potential Ability[COMMENT: contract offer screen - comments table]" />
											</record>
										</record>
									</list>
								</widget>
							</container>					
						</container>
					</container>
				</container>

				<!-- PROS AND CONS -->
				<container class="tcs_box">
					<translation id="title" type="use" value="Pros and Cons" />
					<attachment_group class="vertical_arrange" vertical_alignment="fill" />

					<!-- PROS -->
					<container>
						<attachment_group class="vertical_arrange" vertical_alignment="fill" vertical_gap="8"/>

						<container class="tcs_box" rrep="pro high" transparency="0.35">
							<attachment_group class="vertical_arrange" vertical_alignment="top,extend" />

							<!-- LABEL -->
							<widget class="text" font="title" size="large" colour="positive" auto_size="vertical">
								<translation id="text" translation_id="361260" type="use" />
							</widget>

							<!-- TABLE -->
							<widget class="table" id="tpro" layout="-1" mode="fill_rows" fixed_size_rows="false" auto_size_rows="true" row_spacing="4" default_sort_column="0">
								<list id="column_table_properties">
									<record index="0" sort_ascending="false" />
								</list>
							</widget>

							<!-- NO PROS -->
							<widget class="text" id="nPro" auto_size="vertical" colour="faded text" multiline="true" hidden="true" />
						</container>

						<container class="tcs_box" rrep="con high" transparency="0.35">
							<attachment_group class="vertical_arrange" vertical_alignment="top,extend" />

							<!-- LABEL -->
							<widget class="text" font="title" size="large" colour="negative" auto_size="vertical">
								<translation id="text" translation_id="361094" type="use" />
							</widget>

							<!-- TABLE -->
							<widget class="table" id="tcon" layout="-1" mode="fill_rows" fixed_size_rows="false" auto_size_rows="true" row_spacing="4" default_sort_column="0">
								<list id="column_table_properties">
									<record index="0" sort_ascending="false" />
								</list>
							</widget>

							<!-- NO PROS -->
							<widget class="text" id="nCon" auto_size="vertical" colour="faded text" multiline="true" hidden="true" />
						</container>
					</container>
				</container>
			</container>
		</container>

		<!-- ANALYST REPORT -->
		<container class="player_data_analyst_report_panel" file="none" default_width="-33" priority="2" minimum_width="400">
			<layout class="stick_to_sides_attachment" apply_to_children="true"/>

			<record id="object_property" get_property="objt" set_property="objt"/>

			<!-- ANALYST REPORT -->
			<container class="tcs_box">
				<layout class="stick_to_sides_attachment" apply_to_children="true"/>
				<translation id="title" type="use" translation_id="447607" />

				<!-- No analyst report -->
				<container id="ndar" class="main_box">
					<attachment_group class="vertical_arrange" vertical_alignment="middle" />

					<container id="nocs" height="70">
						<layout class="arrange_vertical_attachment" alignment="top" offset="0" gap="8" />
						<layout class="stick_to_sides_attachment" alignment="horizontal" inset="0" apply_to_children="true" />

						<!--explanatory text-->
						<widget class="label" id="titT" alignment="centre" spec="text" size="11" auto_size="vertical">
							<translation id="text" translation_id="561853" type="use" value="The analysis team will generate a report while a player is being scouted.[COMMENT: AREA: Player profile; SUB-AREA: Analyst Report; FR-BN: FM-14210; NOTE: Explanatory string shown, when there is no analyst report for the player]" />
						</widget>

						<container height="30">
							<layout class="arrange_horizontal_attachment" alignment="middle" offset="0" gap="8" />
							<layout class="stick_to_sides_attachment" alignment="vertical" inset="0" apply_to_children="true" />

							<widget class="button" id="sctP" click_event="sctc" alignment="centre" auto_size="horizontal" height="30">
								<translation id="text" translation_id="243421" type="use" value="Scout Player" />
							</widget>

						</container>
					</container>

					<widget id="nocn" class="label" spec="text" size="11" alignment="centre" auto_size="vertical">
						<layout class="stick_to_sides_attachment" alignment="horizontal" inset="0" />
					</widget>
				</container>

				<!-- IF WE HAVE A REPORT -->
				<container id="pctc">
					<attachment_group class="vertical_arrange" vertical_alignment="bottom,extend" />

					<container class="tcs_tabs" save_session_state="true" save_default_state="true" default_item="daPC">

						<!-- PERFORMANCE GRAPH -->
						<container id="Dper" class="player_scout_reports_panel" file="none">
							<layout class="stick_to_sides_attachment" apply_to_children="true"/>
							<translation id="title" translation_id="538903" type="use" />

							<record id="object_property" get_property="objt" set_property="objt"/>

							<!-- DATA HUB WIDGETS -->
							<container id="dhbW">
								<layout class="stick_to_sides_attachment" alignment="horizontal" inset="0" />
								<layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true" />

								<widget class="player_additional_stats_panel" id="astA" file="player/player additional stats mini report attacking" colour="faded text">
									<record id="object_property" get_property="objt" set_property="objt" />
								</widget>
								<widget class="player_additional_stats_panel" id="asCr" file="player/player additional stats mini report creative" colour="faded text">
									<record id="object_property" get_property="objt" set_property="objt" />
								</widget>
								<widget class="player_additional_stats_panel" id="astP" file="player/player additional stats mini report passing" colour="faded text">
									<record id="object_property" get_property="objt" set_property="objt" />
								</widget>
								<widget class="player_additional_stats_panel" id="astD" file="player/player additional stats mini report defending" colour="faded text">
									<record id="object_property" get_property="objt" set_property="objt" />
								</widget>

								<widget class="data_analyst_visualisation_widget" id="visU" hidden="true" visualisation_type="polygon" data_type="player" category="defensive_wide_midfielder" file="widgets/data analyst visualisation mini" show_mini_visualisation_text="false">
									<record id="object_property" get_property="objt" set_property="objt"/>
									<record id="object_property" get_property="objt" set_property="objt" />
									<record id="object_property" get_property="PclC" set_property="comp" />
								</widget>
							</container>
						</container>

						<!-- PROS -->
						<container id="daPC">
							<translation id="title" type="use" value="Pros and Cons" />
							<attachment_group class="vertical_arrange" vertical_alignment="fill" vertical_gap="8"/>

							<container class="tcs_box" rrep="pro high" transparency="0.35">
								<attachment_group class="vertical_arrange" vertical_alignment="top,extend" />

								<!-- LABEL -->
								<widget class="text" font="title" size="large" colour="positive" auto_size="vertical">
									<translation id="text" translation_id="361260" type="use" />
								</widget>

								<!-- TABLE -->
								<widget class="table" id="tpro" layout="-1" mode="fill_rows" fixed_size_rows="false" auto_size_rows="true" row_spacing="4" default_sort_column="0">
									<list id="column_table_properties">
										<record index="0" sort_ascending="false" />
									</list>
								</widget>

								<!-- NO PROS -->
								<widget class="text" id="nPro" auto_size="vertical" colour="faded text" multiline="true" hidden="true" />
							</container>

							<container class="tcs_box" rrep="con high" transparency="0.35">
								<attachment_group class="vertical_arrange" vertical_alignment="top,extend" />

								<!-- LABEL -->
								<widget class="text" font="title" size="large" colour="negative" auto_size="vertical">
									<translation id="text" translation_id="361094" type="use" />
								</widget>

								<!-- TABLE -->
								<widget class="table" id="tcon" layout="-1" mode="fill_rows" fixed_size_rows="false" auto_size_rows="true" row_spacing="4" default_sort_column="0">
									<list id="column_table_properties">
										<record index="0" sort_ascending="false" />
									</list>
								</widget>

								<!-- NO CONS -->
								<widget class="text" id="nCon" auto_size="vertical" colour="faded text" multiline="true" hidden="true" />
							</container>
						</container>
					</container>

					<!-- SUMMARY -->
					<widget class="text" id="rctw" colour="faded text" auto_size="vertical" />

				</container>
			</container>
		</container>
	</container>

	<!-- IF WE DON'T HAVE PLAYER -->
	<!-- ADAPTIVE CONTAINER -->
	<container class="horizontal_adaptive_container" gap="8">

		<!-- IF HUMAN CONTROLLED TEAM, SHOW -->
		<record id="object_property" get_property="PHcr" set_property="Shwn"/>

		<!-- SCOUT REPORT -->
		<container default_width="-66" class="player_scout_reports_panel" file="none" priority="1">
			<layout class="stick_to_sides_attachment" apply_to_children="true"/>

			<boolean id="use_pro_con_long_strings" value="true"/>

			<record id="object_property" get_property="objt" set_property="objt"/>

			<!-- NO SCOUT REPORT -->
			<container class="tcs_box" id="nsrc">
				<translation id="title" translation_id="316845" type="use" />
				<attachment_group class="vertical_arrange" vertical_alignment="middle" />

				<container id="nocs" height="70">
					<attachment_group class="vertical_arrange_autosize" />

					<!--EXPLANATORY TEXT-->
					<widget class="label" alignment="centre" spec="text" size="11" auto_size="vertical">
						<translation id="text" translation_id="320496" type="use" />
					</widget>

					<!-- BUTTON -->
					<container height="30">
						<attachment_group class="horizontal_arrange" horizontal_alignment="middle" />

						<widget class="button" id="sctP" click_event="sctc" alignment="centre" auto_size="horizontal" height="30">
							<translation id="text" translation_id="243421" type="use"/>
						</widget>
					</container>
				</container>

				<widget id="nocn" class="label" spec="text" size="11" alignment="centre" auto_size="vertical">
					<layout class="stick_to_sides_attachment" alignment="horizontal" inset="0" />
				</widget>
			</container>

			<!-- Already being scounted -->
			<container id="Arbs" class="main_box">
				<layout class="stick_to_sides_attachment" alignment="top, horizontal" apply_to_children="true" inset="0" />

				<!--explanatory text-->
				<widget class="text" id="arbt" alignment="centre" spec="text" colour="attention" height="80">
					<translation id="text" translation_id="447600" type="use" value="This player is being scouted by [%person#1] and the report will be available shortly.[COMMENT:scout report, scouting in progress]" />
				</widget>
			</container>

			<!-- Already being scouted by pool -->
			<container id="Pool" class="main_box">
