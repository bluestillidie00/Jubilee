<panel>
	<layout class="stick_to_sides_attachment" apply_to_children="true"/>

	<container class="horizontal_adaptive_container" gap="8">

		<!-- LAST MATCH -->
		<!-- COMMENTED OUT AS IT ONLY WORKS IF YOU HAVE TEAM REPORT 
				<container default_width="480" class="tcs_box" priority="2">
					<layout class="stick_to_sides_attachment" apply_to_children="true"/>
					<translation id="title" translation_id="316719" type="use" />

					<container class="team_container_information_overview_panel" file="none">
						<layout class="stick_to_sides_attachment" apply_to_children="true"/>
						<record id="object_property" get_property="objt" set_property="objt"/>

						<container class="client_object_viewer_selector_panel" file="club/tcs/overview/overview/last_match" id="cos1" priority="1" default_widget="clod" late_loading="true" dont_set_embedded_title="true"/>
					</container> 
				</container> -->

		<!-- LEFT -->
		<container default_width="550" priority="4">
			<layout class="stick_to_sides_attachment" apply_to_children="true"/>

			<container class="vertical_adaptive_container">

				<!-- RESULTS -->
				<container class="club_overview_team_results_panel" file="club/tcs/overview/overview/fixtures/club overview team calendar results" default_height="180">
					<record id="object_property" get_property="objt" set_property="objt"/>
				</container>

				<!-- FIXTURES -->
				<container class="club_overview_team_results_panel" file="club/tcs/overview/overview/fixtures/fixtures table" default_height="-12" priority="2">
					<record id="object_property" get_property="objt" set_property="objt"/>
				</container>
				
				<container class="tcs_box" default_height="330" minimum_height="300" priority="1">
					<layout class="stick_to_sides_attachment" apply_to_children="true"/>
				
					<!-- TABS -->
					<container class="tcs_tabs" id="tSta" default_item="lgbt" save_default_state="true" save_session_state="true">

						<!-- LAST XI -->
						<container class="team_container_information_overview_panel" id="csio" file="club/tcs/overview/overview/fixtures/last xi/pitch">
							<translation id="title" translation_id="447658" type="use"/>
							<layout class="stick_to_sides_attachment" apply_to_children="true"/>
							<record id="object_property" get_property="objt" set_property="objt"/>
						</container>

						<!-- LEAGUE TABLE -->
						<container id="lgbt">
							<layout class="vertical_arrange" vertical_alignment="top,extend" vertical_gap="0"/>
							<translation id="title" type="use" value="League Table"/>

							<container class="competition_news_panel" file="none">
								<attachment_group class="vertical_arrange" vertical_alignment="bottom,extend"/>

								<record id="object_property" get_property="objt" set_property="objt"/>

								<container class="competition_overview_panel" file="none">
									<layout class="stick_to_sides_attachment" alignment="all" apply_to_children="true"/>

									<record id="object_property" get_property="Cdiv" set_property="objt"/>

									<attachment_group class="horizontal_arrange" horizontal_layout="-1,-2" horizontal_offset="0" vertical_inset="0">
										<global id="horizontal_gap" name="ui.main_contrast_box_gap"/>
									</attachment_group>

									<container id="cost" navigation_container="true" default_focus="false">
										<layout class="stick_to_sides_attachment" apply_to_children="true"/>

										<widget class="competition_stage_panel" file="none">
											<record id="object_property" get_property="objt" set_property="objt"/>

											<layout class="arrange_vertical_attachment" alignment="bottom,extend" offset="0"/>
											<layout class="stick_to_sides_attachment" alignment="horizontal" apply_to_children="true" inset="0"/>

											<!-- the group box that is the main container for the competition stage panel
			this container holds whatever league or cup panel we need to create -->
											<container class="main_box" id="stse">

												<!-- bottom row of the stage panel is used to store cup draw buttons -->
												<layout class="arrange_vertical_attachment" alignment="bottom,extend" offset="0"/>
												<layout class="stick_to_sides_attachment" alignment="horizontal" apply_to_children="true" inset="0"/>

												<!-- No game panel-->
												<container id="nots" hidden="true">
													<layout class="arrange_vertical_attachment" alignment="top" offset="0" gap="0"/>
													<layout class="stick_to_sides_attachment" alignment="horizontal" apply_to_children="true" inset="200"/>
													<layout class="stick_to_sides_attachment" alignment="all" inset="0"/>

													<widget class="spacer" height="50"/>

													<widget class="text" id="nost" alignment="centre" spec="text" size="large" style="semi_bold, embossed" auto_size="vertical"/>
												</container>

												<!-- group box for stage to go into -->
												<container id="stgb">
													<layout class="stick_to_sides_attachment" alignment="all" apply_to_children="true"/>

													<record id="default_properties" hidden="true"/>

													<!--league stage panel-->
													<widget class="league_stage_panel" id="lspn" save_session_state="true" should_force_refresh="true"/>

													<!--competition rules panel-->
													<widget class="rules_panel" id="rupa"/>

													<!--cup stage_panel-->
													<widget class="cup_stage_panel" id="cstp" save_session_state="true"/>

													<!--cup tree panel-->
													<widget class="cup_tree_panel" id="ctpn" save_session_state="true"/>

													<!--group stage scrolling box (to be filled in code)-->
													<widget class="scrolling_box" id="gscb" save_session_state="true" navigation_container="true" navigation_container_allow_empty="true"/>

													<!-- media prediction -->
													<container class="media_predicition_league_table_panel" id="odds"/>
												</container>

												<!--xG table for league-->
												<widget class="client_object_list_table" hidden="true" id="xgtb" mode="fill_rows, stripe_rows">
													<list id="default_sort_column">
														<integer value="sxGP"/>
													</list>

													<list id="column_widget_properties">
														<record index="10" alignment="centre"/>
													</list>

													<record id="viewed_property_ids">
														<record id="Tpos" alignment="centre"/>
														<record id="xGPo"/>
														<record id="xPOd"/>
														<record id="Ttea"/>
														<record id="svxG"/>
														<record id="sxGA"/>
														<record id="sxGP"/>
													</record>
												</widget>

												<container navigation_container="true">

													<attachment_group class="vertical_arrange_autosize" vertical_offset="0" horizontal_alignment="horizontal" horizontal_inset="0"/>

													<!-- The exceptions so point deductions in the league etc-->
													<widget class="table" layout="-1" id="exta" auto_size="vertical" row_height="22" hidden="true" mode="fill_rows"/>

													<!-- group box for buttons -->
													<container id="stbt">
														<layout class="fit_children_attachment" alignment="vertical" offset="2"/>
														<layout class="arrange_horizontal_attachment" alignment="right" offset="0"/>
														<layout class="stick_to_sides_attachment" alignment="top" apply_to_children="true" inset="0"/>

														<record id="default_properties" auto_size="all"/>

														<!-- 4 cup action buttons -->
														<widget class="button" id="dnte" click_event="dnte">
															<translation id="text" translation_id="247176" type="use" value="Draw Next Team"/>
														</widget>
														<widget class="icon_button" id="datm" click_event="datm" controller_button="y_button">
															<translation id="text" translation_id="247177" type="use" value="Draw All Teams"/>
														</widget>
														<widget class="icon_button" id="audr" click_event="audr" controller_button="x_button">
															<translation id="text" translation_id="247178" type="use" value="Automatic Draw"/>
														</widget>
														<widget class="icon_button" id="sadr" click_event="sadr" controller_button="x_button">
															<translation id="text" translation_id="247179" type="use" value="Stop Automatic Draw"/>
														</widget>
													</container>
												</container>
											</container>

											<!-- teams left to draw for a cup stage -->
											<widget class="comp_teams_to_draw_panel" id="ctdp" hidn="true"/>

											<!-- league stage fixtures-->
											<widget class="competition_fixtures_panel" id="cmfp" height="370" display_mode="1" group_sort_human_fixtures="false" show_view_stage_link="false">
												<attachment class="test_screen_size_attachment" skip_if_false="true">
													<list id="properties">
														<record>
															<flags id="alignment" value="vertical"/>
															<integer id="min_value" value="0"/>
															<integer id="max_value" value="768"/>
															<symbol id="set_property" value="height"/>
															<integer id="true_value" value="285"/>
															<integer id="false_value" value="370"/>
														</record>
													</list>
												</attachment>
											</widget>

										</widget>
									</container>
								</container>
							</container>
						</container>


						<!-- PLAYER STATS -->
						<container class="team_container_information_overview_panel" file="none" id="pssp">
							<layout class="stick_to_sides_attachment" apply_to_children="true"/>
							<translation id="title" translation_id="248440" type="use" value="Player Stats"/>

							<record id="object_property" get_property="objt" set_property="objt"/>

							<container>
								<layout class="stick_to_sides_attachment" apply_to_children="true"/>
								<record id="default_properties" default_width="-1" minimum_width="250"/>

								<!--snapshot selector 1-->
								<container class="client_object_viewer_selector_panel" file="club/tcs/overview/overview/fixtures/dummysel_2" id="cos1" priority="1" default_widget="pssp" late_loading="true" save_session_state="true" dont_set_embedded_title="true"/>
							</container>
						</container>

						<!-- TEAM STATS -->
						<container class="team_container_information_overview_panel" file="none" id="tssp">
							<layout class="stick_to_sides_attachment" apply_to_children="true"/>
							<translation id="title" translation_id="247393" type="use" value="Team Stats"/>

							<record id="object_property" get_property="objt" set_property="objt"/>

							<container>
								<layout class="stick_to_sides_attachment" apply_to_children="true"/>
								<record id="default_properties" default_width="-1" minimum_width="250"/>

								<!--snapshot selector 1-->
								<container class="client_object_viewer_selector_panel" file="club/tcs/overview/overview/fixtures/dummysel_1" id="cos1" priority="1" default_widget="pssp" late_loading="true" save_session_state="true" dont_set_embedded_title="true"/>
							</container>
						</container>
					</container>
				</container>
			</container>
		</container>

		<!-- MIDDLE -->
		<container default_width="-1" class="tcs_box" minimum_width="400" priority="1">
			<layout class="stick_to_sides_attachment" apply_to_children="true"/>

			<container class="vertical_adaptive_container" gap="8">

				<!-- STADIUM -->
				<container default_height="360" prioity="-1">
					<layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true"/>

					<widget class="club_overview_stadium_panel" id="clos" file="club/tcs/overview/overview/stadium">
						<record id="object_property">
							<list id="get_properties">
								<record>
									<integer id="get_property" value="TsHA"/>
									<integer id="set_property" value="Shwn"/>
									<boolean id="dont_set_object" value="true"/>
								</record>
								<record>
									<integer id="get_property" value="objt"/>
									<integer id="set_property" value="Cclu"/>
									<boolean id="dont_set_object" value="true"/>
								</record>
								<record>
									<integer id="get_property" value="Tsre"/>
									<integer id="set_property" value="objt"/>
									<boolean id="dont_set_hint" value="true"/>
								</record>
							</list>
						</record>
					</widget>

					<!-- NO STADIUM INFO -->
					<widget class="text" id="NoSt" alignment="centre">
						<translation id="text" translation_id="436452" type="use" value="Stadium information not available.[COMMENT: line of text saying when we don't have any information on a team's stadium]"/>
						<record id="object_property">
							<integer id="get_property" value="TsHA"/>
							<integer id="set_property" value="hidn"/>
						</record>
					</widget>
				</container>

				<!-- FIXTURES AND RESULTS -->
				<container default_height="-1" priority="2">
					<attachment_group class="vertical_arrange" vertical_alignment="fill" vertical_gap="8"/>

					<widget class="club_overview_kits_panel">
						<record id="object_property" get_property="objt" set_property="objt"/>
					</widget>

				</container>

			</container>
		</container>

		<!-- RIGHT -->
		<container default_width="550" class="tcs_box" priority="2">
			<layout class="stick_to_sides_attachment" apply_to_children="true"/>

			<container class="vertical_adaptive_container" gap="8">

				<!-- STAFF -->
				<container default_height="288" minimum_height="200" priority="1">
					<attachment_group class="vertical_arrange" vertical_alignment="top,extend"/>

					<!-- TCS HEADER -->
					<widget class="tcs_header" section="tcsp">
						<translation id="text" type="use" translation_id="509699"/>
					</widget>

					<widget class="club_overview_staff_panel" id="keys" late_loading="true" file="club/tcs/overview/overview/staff overview">
						<record id="object_property" get_property="objt" set_property="objt"/>
					</widget>
				</container>

				<!-- FACILITIES -->
				<container>
					<attachment_group class="vertical_arrange_autosize"/>

					<!-- TCS HEADER -->
					<widget class="tcs_header" section="">
						<translation id="text" type="use" translation_id="292709"/>
					</widget>

					<container>
						<attachment_group class="vertical_arrange_autosize"/>

						<record id="default_properties" height="44"/>

						<!-- TRAINING -->
						<container class="client_object_viewer_xml_panel" file="club/tcs/overview/overview/facilities/training">
							<record id="object_property" get_property="objt" set_property="objt"/>
						</container>

						<!-- YOUTH -->
						<container class="client_object_viewer_xml_panel" file="club/tcs/overview/overview/facilities/youth">
							<record id="object_property" get_property="objt" set_property="objt"/>
						</container>

						<!-- COACHING -->
						<container class="client_object_viewer_xml_panel" file="club/tcs/overview/overview/facilities/coaching">
							<record id="object_property" get_property="objt" set_property="objt"/>
						</container>

						<!-- RECRUITMENT -->
						<container class="client_object_viewer_xml_panel" file="club/tcs/overview/overview/facilities/recruitment">
							<record id="object_property" get_property="objt" set_property="objt"/>
						</container>
					</container>
				</container>

				<container default_height="-1">
					<attachment_group class="vertical_arrange" vertical_alignment="top,extend"/>

					<!-- TCS HEADER -->
					<widget class="tcs_header" section="">
						<translation id="text" type="use" translation_id="364723"/>
					</widget>
					<container class="tcs_subtle_box">
						<layout class="arrange_horizontal_attachment" layout="-1,-1" offset="0" gap="8"/>
						<layout class="stick_to_sides_attachment" alignment="vertical" inset="0" apply_to_children="true"/>

						<!--rivals-->
						<container class="scrolling_box">

							<container>
								<attachment_group class="vertical_arrange_autosize" vertical_offset="0" horizontal_inset="0"/>

								<!--fierce rivals-->
								<widget class="label" id="CfmL" spec="text" size="small" auto_size="vertical" alignment="left,bottom" style="semi_bold">
									<record id="object_property">
										<integer id="get_property" value="Cfmr"/>
										<boolean id="name" value="true"/>
									</record>
								</widget>

								<widget class="club_rivalries_list_text" id="CfmV" spec="text" auto_size="vertical" alignment="left,top">
									<record id="object_property">
										<integer id="get_property" value="Cfmr"/>
									</record>
								</widget>

								<!--other rivals-->
								<widget class="label" id="CfrL" spec="text" size="small" auto_size="vertical" alignment="left,bottom" style="semi_bold">
									<record id="object_property">
										<integer id="get_property" value="Cfrt"/>
										<boolean id="name" value="true"/>
									</record>
								</widget>

								<widget class="club_rivalries_list_text" id="CfrV" spec="text" auto_size="vertical" alignment="left,top">
									<record id="object_property">
										<integer id="get_property" value="Cfrt"/>
									</record>
								</widget>

							</container>
						</container>

						<!--derbies-->
						<container class="scrolling_box">

							<container>
								<attachment_group class="vertical_arrange_autosize" vertical_offset="0" horizontal_inset="0"/>

								<!--derbies-->
								<widget class="label" id="TDeL" spec="text" size="small" auto_size="vertical" alignment="left,bottom" style="semi_bold">
									<record id="object_property">
										<integer id="get_property" value="TDer"/>
										<boolean id="name" value="true"/>
									</record>
								</widget>

								<widget class="team_derbies_text" id="TDeV" spec="text" auto_size="vertical" alignment="left,top">
									<record id="object_property">
										<integer id="get_property" value="TDer"/>
									</record>
								</widget>

							</container>
						</container>
					</container>
				</container>


			</container>
		</container>
	</container>
</panel>